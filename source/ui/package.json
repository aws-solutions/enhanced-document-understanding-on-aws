{
  "name": "intelligent-document-processing-ui",
  "version": "1.0.0",
  "dependencies": {
    "@aws-amplify/ui-react": "^5.0.7",
    "@cloudscape-design/components": "^3.0.327",
    "@cloudscape-design/design-tokens": "^3.0.23",
    "@cloudscape-design/global-styles": "^1.0.12",
    "@types/react-pdf": "^6.2.0",
    "aws-amplify": "^5.3.6",
    "bootstrap": "^5.3.1",
    "dayjs": "^1.11.9",
    "pdfjs-dist": "2.16.105",
    "ramda": "^0.29.0",
    "react": "^18.2.0",
    "react-bootstrap": "^2.8.0",
    "react-dom": "^18.2.0",
    "react-pdf": "^6.2.2",
    "react-router-dom": "^6.14.2",
    "react-tabs": "^6.0.2",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@cloudscape-design/jest-preset": "^2.0.17",
    "@tanstack/react-table": "^8.9.3",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/react-test-renderer": "^18.0.0",
    "@types/jest": "^29.5.3",
    "@types/uuid": "^9.0.2",
    "babel-jest": "^29.6.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.6.2",
    "react-scripts": "^5.0.1",
    "react-test-renderer": "^18.2.0"
  },
  "overrides": {
    "nth-check": "^2.0.1"
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!@cloudscape-design)/"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "collectCoverageFrom": [
      "**.js",
      "**.tsx",
      "**.ts",
      "**.jsx",
      "!src/__test__/**",
      "!coverage/**",
      "!test/*.js",
      "!jest.config.js",
      "!src/App.js",
      "!src/index.js"
    ]
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "cp ./node_modules/pdfjs-dist/build/pdf.worker.min.js public/ && react-scripts build",
    "test": "react-scripts test --coverage --watchAll=false --silent",
    "test-debug": "react-scripts --inspect-brk test --runInBand --no-cache",
    "eject": "react-scripts eject",
    "clean": "rm -rf node_modules",
    "code-formatter": "./node_modules/prettier/bin-prettier.js --config ../../.prettierrc.yml '**/*.{js,json,css,md}' !package*.json --write",
    "code-linter": "./node_modules/eslint/bin/eslint.js . -c ../.eslintrc.js --ext .js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "description": "React application for UI",
  "license": "Apache-2.0",
  "author": {
    "name": "Amazon Web Services",
    "url": "https://aws.amazon.com/solutions"
  }
}